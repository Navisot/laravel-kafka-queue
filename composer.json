{
    "name": "navisot/laravel-kafka-queue",
    "description": "A mini package to use Kafka as a laravel driver.",
    "require": {
        "php": ">=8.1",
        "ext-rdkafka": "*",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "illuminate/queue": "^10.0 || ^11.0 || ^12.0",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Kafka\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kafka\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "authors": [
        {
            "name": "Nick Avr",
            "email": "nikosavr89@gmail.com"
        }
    ]
}
